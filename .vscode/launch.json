    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
            },
        },
        {
            "name": "Python: main.py fit ANORAK",
            "type": "debugpy",
            "request": "launch",
            "program": "main.py",
            "args": [
                "fit",
                "-c",
                "/home/valentin/workspaces/benchmark-vfm-ss/configs/anorak_linear_semantic_debug.yaml",
                "--root",
                "data/",
                "--no_compile"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "CUDA_VISIBLE_DEVICES": "1",
                "HYDRA_FULL_ERROR": "1"
            },
            "justMyCode": false
        }
    ]
}